{
    "name": "test-treeshaking",
    "type": "module",
    "sideEffects": false,
    "version": "1.3.1-0",
    "files": [
      "dist",
      "dist/*.d.ts"
    ],
    "main": "./dist/design-system-u.umd.js",
    "module": "./dist/design-system-u.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/design-system-u.es.js",
        "require": "./dist/design-system-u.umd.js",
        "types": "./dist/index.d.ts"
      },
      "./dist/style.css": "./dist/style.css"
    },
    "scripts": {
      "dev": "vite",
      "build": "rimraf dist && node --max_old_space_size=8192 ./node_modules/vite/bin/vite.js build",
      "preview": "vite preview --port 5050",
      "typecheck": "vue-tsc --emitDeclarationOnly",
      "storybook": "storybook dev -p 6006",
      "build-storybook": "storybook build",
      "serve-storybook": "npx http-server storybook-static",
      "deploy-storybook": "gh-pages -d storybook-static -t true && rm -rf storybook-static",
      "build-deploy-storybook": "storybook build && gh-pages -d storybook-static -t true",
      "chromatic": "storybook build && npx chromatic --project-token=CHROMATIC_PROJECT_TOKEN --storybook-build-dir storybook-static",
      "generate-changelog": "node ./node_modules/gitmoji-changelog/src/index.js",
      "lint": "eslint . --ext .vue,.ts,.js",
      "lint:fix": "eslint . --ext .vue,.ts,.js --fix",
      "test:unit": "vitest run --coverage",
      "test:component": "cypress run --component --config video=false -b chrome",
      "new-component": "node ./tools/create-component/create-component.js",
      "cy:open": "cypress open",
      "serve:TI": "vite --port 8080 --host localhost"
    },
    "dependencies": {
      "@fontsource/open-sans": "^4.5.13",
      "@fontsource/roboto-mono": "^5.0.8",
      "@fontsource/roboto-slab": "^4.5.9",
